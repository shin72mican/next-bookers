{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
     "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false
    },
    "db:migrate": {
      "cache": false,
      "persistent": true
    },
    "db:deploy": {
      "cache": false
    },
    "lint": {},
    "lint:fix": {},
    "typecheck": {}
  }
}